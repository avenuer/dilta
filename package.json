    "e:pack": "npx webpack-cli --config=./configs/webpack.electron.js",
    "e:launch": "cross-env-shell ELECTRON_IS_DEV=development \"npm run e:pack && npm run e:start\"",
    "e:rebuild": "npx electron-rebuild",
    "e:package": "rimraf build/package && npx electron-builder",
    "e:build:package": "cross-env-shell ELECTRON_IS_DEV=development \"npm run e:pack && e:package\"",
    "e:build:ship": "cross-env-shell PLATFORM=desktop ELECTRON_IS_DEV=production \"npm run e:pack && e:package\"",
    "e:build:cli": "pkg -t node6-win ./dist/cli.js -o ./build/cli",
