{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dilta-web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/dilta-web",
      "sourceRoot": "projects/dilta-web/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/dilta-web",
            "index": "projects/dilta-web/src/index.html",
            "main": "projects/dilta-web/src/main.ts",
            "polyfills": "projects/dilta-web/src/polyfills.ts",
            "tsConfig": "projects/dilta-web/tsconfig.app.json",
            "assets": [
              "projects/dilta-web/src/favicon.ico",
              "projects/dilta-web/src/assets"
            ],
            "styles": [
              "projects/dilta-web/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/dilta-web/src/environments/environment.ts",
                  "with": "projects/dilta-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dilta-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dilta-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dilta-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dilta-web/src/test.ts",
            "polyfills": "projects/dilta-web/src/polyfills.ts",
            "tsConfig": "projects/dilta-web/tsconfig.spec.json",
            "karmaConfig": "projects/dilta-web/karma.conf.js",
            "assets": [
              "projects/dilta-web/src/favicon.ico",
              "projects/dilta-web/src/assets"
            ],
            "styles": [
              "projects/dilta-web/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dilta-web/tsconfig.app.json",
              "projects/dilta-web/tsconfig.spec.json",
              "projects/dilta-web/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/dilta-web/e2e/protractor.conf.js",
            "devServerTarget": "dilta-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dilta-web:serve:production"
            }
          }
        }
      }
    },
