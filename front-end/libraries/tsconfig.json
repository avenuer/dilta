{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "admin-ui": [
        "dist/admin-ui"
      ],
      "admin-ui/*": [
        "dist/admin-ui/*"
      ],
      "auth-ui": [
        "dist/auth-ui"
      ],
      "auth-ui/*": [
        "dist/auth-ui/*"
      ],
      "datagrid": [
        "dist/datagrid"
      ],
      "datagrid/*": [
        "dist/datagrid/*"
      ],
      "academic-ui": [
        "dist/academic-ui"
      ],
      "academic-ui/*": [
        "dist/academic-ui/*"
      ],
      "auth-ngrx-store": [
        "dist/auth-ngrx-store"
      ],
      "auth-ngrx-store/*": [
        "dist/auth-ngrx-store/*"
      ],
      "school-ngrx-store": [
        "dist/school-ngrx-store"
      ],
      "school-ngrx-store/*": [
        "dist/school-ngrx-store/*"
      ],
      "settings-ui": [
        "dist/settings-ui"
      ],
      "settings-ui/*": [
        "dist/settings-ui/*"
      ],
      "web-transport": [
        "dist/web-transport"
      ],
      "web-transport/*": [
        "dist/web-transport/*"
      ]
    }
  }
}